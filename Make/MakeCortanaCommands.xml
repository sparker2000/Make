<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
   <CommandSet xml:lang="en-us" Name="MakeCommandSet_en-us">
      <CommandPrefix> Make, </CommandPrefix>
      <Example>Make</Example>

      <Command Name="sayHello">
         <Example> Hello </Example>
         <ListenFor> Hello Cortana </ListenFor>
         <Feedback> Hello Joseph. </Feedback>
         <Navigate/>
      </Command>

      <Command Name="LedState">
         <Example> Make LED 5 turn on </Example>
         <ListenFor> LED {Number} {State} </ListenFor>
         <Feedback> Making LED {Number} {State}. </Feedback>
         <Navigate/>
      </Command>
      
      <!--State to set the LEDs-->
      <PhraseList Label="State">
         <Item>turn on</Item>
         <Item>turn off</Item>
         <Item>Toggle</Item>
      </PhraseList>

      <!--Valid LED Numbers for a Raspberry Pi 2 Model B-->
      <PhraseList Label="Number">
         <Item>two</Item>
         <Item>three</Item>
         <Item>four</Item>
         <Item>five</Item>
         <Item>six</Item>
         <Item>seven</Item>
         <Item>eight</Item>
         <Item>nine</Item>
         <Item>ten</Item>
